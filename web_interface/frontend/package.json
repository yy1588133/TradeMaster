{
  "name": "trademaster-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "TradeMaster Web Interface Frontend",
  "author": "TradeMaster Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "build:report": "tsc && vite build && npx vite-bundle-analyzer",
    "preview": "vite preview --host",
    "preview:dist": "vite preview --host --port 4173",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:ci": "eslint . --ext ts,tsx --format junit --output-file reports/eslint-report.xml",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,less,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,less,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=reports/junit.xml",
    "clean": "rimraf dist node_modules/.cache .eslintcache",
    "clean:all": "rimraf dist node_modules node_modules/.cache .eslintcache",
    "deps:check": "pnpm audit && pnpm outdated",
    "deps:update": "pnpm update --interactive --latest",
    "prepare": "husky install",
    "size": "size-limit",
    "analyze": "pnpm build:analyze && npx vite-bundle-analyzer dist/stats.html",
    "start": "pnpm dev",
    "serve": "pnpm preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "antd": "^5.12.8",
    "@ant-design/icons": "^5.2.6",
    "@ant-design/plots": "^2.0.2",
    "@ant-design/pro-components": "^2.6.48",
    "@ant-design/pro-layout": "^7.17.16",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "classnames": "^2.3.2",
    "ahooks": "^3.7.8",
    "immer": "^10.0.3",
    "query-string": "^8.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "less": "^4.2.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "jsdom": "^23.0.1",
    "happy-dom": "^12.10.3",
    "msw": "^2.0.8",
    "@types/testing-library__jest-dom": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "rimraf": "^5.0.5",
    "size-limit": "^10.0.2",
    "@size-limit/preset-app": "^10.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "terser": "^5.24.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mock": "^3.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "@vitejs/plugin-legacy": "^5.0.0",
    "lightningcss": "^1.22.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.10.0",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,less,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "size-limit": [
    {
      "name": "Bundle Size",
      "path": "dist/assets/*.js",
      "limit": "1000 KB"
    },
    {
      "name": "CSS Size",
      "path": "dist/assets/*.css",
      "limit": "100 KB"
    }
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeMaster-NTU/TradeMaster.git",
    "directory": "web_interface/frontend"
  },
  "bugs": {
    "url": "https://github.com/TradeMaster-NTU/TradeMaster/issues"
  },
  "homepage": "https://trademaster.ai"
}