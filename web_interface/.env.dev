# ==================== TradeMaster Web Interface 开发环境配置 ====================
# 基础项目配置
PROJECT_NAME=TradeMaster Web Interface (Dev)
VERSION=1.0.0-dev
DEBUG=true
LOG_LEVEL=DEBUG
NODE_ENV=development
ENVIRONMENT=development

# 服务器端口配置
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
BACKEND_PORT=8000
BACKEND_DEBUG_PORT=5678
FRONTEND_PORT=3100
NGINX_PORT=8080

# 数据库配置
POSTGRES_SERVER=postgres
POSTGRES_USER=trademaster
POSTGRES_PASSWORD=dev_password_123
POSTGRES_DB=trademaster_web
POSTGRES_PORT=5432

# Redis缓存配置
REDIS_HOST=redis
REDIS_PORT=6380
REDIS_DB=0
REDIS_PASSWORD=dev_redis_123

# 安全和认证配置
SECRET_KEY=dev-secret-key-change-in-production-this-is-only-for-development-use
ACCESS_TOKEN_EXPIRE_MINUTES=60
ALGORITHM=HS256

# CORS配置
BACKEND_CORS_ORIGINS=http://localhost:3100,http://localhost:8080,http://127.0.0.1:3100

# TradeMaster集成配置
TRADEMASTER_API_URL=http://host.docker.internal:8080
TRADEMASTER_DATA_PATH=/app/trademaster_data

# 前端应用配置
REACT_APP_API_BASE_URL=http://localhost:8000/api/v1
REACT_APP_WS_URL=ws://localhost:8000/ws
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true

# 开发配置
AUTO_RELOAD=true
SHOW_ERROR_DETAILS=true
ENABLE_DOCS=true
CHOKIDAR_USEPOLLING=true
WATCHPACK_POLLING=true

# 构建配置
BUILD_ENV=development
PIP_INDEX_URL=https://pypi.org/simple/

# 开发工具配置
PGADMIN_EMAIL=admin@trademaster.dev
PGADMIN_PASSWORD=admin123
PGADMIN_PORT=5050
REDIS_COMMANDER_USER=admin
REDIS_COMMANDER_PASSWORD=admin123
REDIS_COMMANDER_PORT=8081
MAILHOG_WEB_PORT=8025
MAILHOG_SMTP_PORT=1025

# 时区配置
TZ=Asia/Shanghai

# Docker配置
PWD=.